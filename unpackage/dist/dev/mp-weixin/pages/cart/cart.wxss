._ul {
  list-style: none;
  padding: 0;
  margin: 0;
}
._a {
  color: #555;
  text-decoration: none;
}
._a:hover {
  color: #f30;
  text-decoration: none;
}
.container {
  width: 100%;
  margin: 0 auto;
}
.cart {
  margin-top: 20px;
}
.cart_header {
  text-align: right;
  font-size: 28rpx;
  line-height: 72rpx;
}
.cart_header > button {
  display: inline-block;
  vertical-align: middle;
  width: 200rpx;
  height: 72rpx;
  background: #f63;
  border: none;
  color: #fff;
  font-size: 36rpx;
}
.cart_header > ._span {
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
}
.cart_header > ._span > ._em {
  font-style: normal;
  color: #f63;
}
.cart_header > ._span > ._strong {
  font-size: 36rpx;
  color: #f63;
}
.cart_header > ._p {
  float: left;
  width: 100%;
  text-align: center;
}
.cart_con {
  margin: 10px 0;
  font-size: 28rpx;
}
/*购物车列表*/
.cartcon_list ._h2 {
  line-height: 120rpx;
  text-align: center;
  font-size: 36rpx;
  font-weight: lighter;
  border: 1px solid #ddd;
  border-top: none;
}
.cartcon_list ._h2 ._a {
  color: #f60;
}
.cartcon_list ._ul ._li {
  padding: 10px 0;
  border: 1px solid #ddd;
  border-top: none;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cartcon_list > ._ul > ._li > ._span,
.cartcon_list > ._ul > ._li > ._a,
.cartcon_list > ._ul > ._li > ._i,
.cartcon_list > ._ul > ._li > view,
.cartcon_list > ._ul > ._li > view > ._span,
.cartcon_list > ._ul > ._li > view > input,
.cartcon_list > ._ul > ._li > ._strong,
.cartcon_list > ._ul > ._li > ._em {
  display: inline-block;
  vertical-align: middle;
  text-align: center;
  font-style: normal;
}
.cartcon_list > ._ul > ._li > ._span {
  width: 60rpx;
}
.cart_img {
  display: block;
  overflow: hidden;
  margin-top: 20rpx;
}
.cart_img image {
  width: 100%;
  height: 100%;
}
.cart_title {
  width: 180rpx;
  font-size: 18rpx;
  margin-top: 10rpx;
  text-align: left;
  display: block;
}
.cartcon_list > ._ul > ._li > ._i {
  width: 200rpx;
}
.cartcon_list > ._ul > ._li > view {
  width: 240rpx;
}
.cartcon_list > ._ul > ._li > view > ._span {
  width: 40rpx;
  height: 40rpx;
  background: #e8e8e8;
  line-height: 40rpx;
}
.cartcon_list > ._ul > ._li > view > ._span:hover {
  cursor: pointer;
  background: #eee;
}
.cartcon_list > ._ul > ._li > view > input {
  width: 56rpx;
  height: 36rpx;
  border: 1px solid #ccc;
}
.cartcon_list > ._ul > ._li > ._strong {
  width: 200rpx;
  color: #f63;
}
.cartcon_list > ._ul > ._li > ._em {
  margin-left: 20px;
  width: 28rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat 0 -920rpx;
  cursor: pointer;
}
.cartcon_list > ._ul > ._li > ._em:hover {
  background-position: 0 -920rpx;
}
.cart_checkbox {
  margin-left: 10rpx;
}

